<div class="likeIndex">
  <div class="text">
    <h1>★&nbsp;マークした単語一覧&nbsp;★</h1>
    <h2>重点的に覚えよう！</h2>
  </div>
  <div class="card1">
    <% @like_list.each do |l| %>
      <div class="card2">
        <%= l.front %>&nbsp;:&nbsp;
        <%= l.back %>
      </div>
    <% end %>
  </div>
  <%= will_paginate @like_list,
  class: "my-pagination-class",
  params: {
    controller: :likes,
    action: :index
  } %>
</div>
