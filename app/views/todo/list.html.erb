<h1>こちら、todoリスト作成ページです</h1>
<%= link_to "リストを作成する", new_list_path%>
<% @lists.each do |list| %>
      <div class="list">
        <div class="list_header">
          <h2 class="list_header_title"><%= list.title %></h2>
          <%= link_to "編集する", edit_list_path(list) %>
          <%= link_to "削除する", list, data: {
        "turbo-method": :delete,
        turbo_confirm: "You sure?"
      } %>
        </div>
      </div>
<% end %>
